{
  "name": "svelte_capacitor",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build:app": "svelte-kit build",
    "build:android": "run-s build:app cap-run:android",
    "build:ios": "run-s build:app cap-run:ios",
    "cap-run:android": "cap sync android && cap open android",
    "cap-run:ios": "cap sync ios && cap open ios",
    "dev": "svelte-kit dev",
    "dev:preview": "svelte-kit preview",
    "dev:ios": "run-p dev cap-run:ios",
    "dev:android": "run-p dev cap-run:android",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@capacitor-community/http": "^1.3.0",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.3.6",
    "cssnano": "^5.0.8",
    "daisyui": "^1.14.2",
    "date-fns": "^2.24.0",
    "date-fns-tz": "^1.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.8",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "safe-area-insets": "^1.4.1",
    "svelte": "^3.43.1",
    "svelte-check": "^2.2.6",
    "svelte-i18n": "^3.3.12",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.16",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@capacitor/android": "^3.2.4",
    "@capacitor/cli": "^3.2.4",
    "@capacitor/core": "^3.2.4",
    "@capacitor/ios": "^3.2.4"
  }
}